{
  "name": "keltiecochrane/themosis-juicer",
  "description": "A Themosis WordPress plugin that provides a wrapper for the Juicer.io API.",
  "keywords": ["themosis", "plugin"],
  "authors": [
    {
      "name": "Daniel Gadd",
      "email": "daniel@djgadd.uk",
      "homepage": "http://djgadd.uk"
    },
    {
      "name": "Keltie Cochrane Ltd.",
      "email": "digital@keltiecochrane.com",
      "homepage": "https://keltiecochrane.com"
    }
  ],
  "type": "wordpress-plugin",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "git@github.com:djgadd/framework.git"
    }
  ],
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.2",
    "keltiecochrane/themosis-cache": "^0.1.0",
    "keltiecochrane/themosis-illuminate": "^0.0.1",
    "guzzlehttp/guzzle": "^6.2",
    "illuminate/support": "^5.4",
    "nesbot/carbon": "^1.22"
  },
  "require-dev": {
    "themosis/framework": "dev-feature/issue_372"
  },
  "extra": {
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"]
    }
  }
}
